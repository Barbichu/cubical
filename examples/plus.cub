primitive
  Id   : (A : U) (a b : A) -> U

data N = zero | suc (n : N)

idnat : N -> N
idnat = split
  zero -> $ zero
  suc n -> $ suc (idnat n)

plus : N -> N -> N
plus = split
  zero -> \m -> m
  suc n -> \m -> $ suc (plus n m)

two : N
two = $ suc ($ suc ($ zero))
